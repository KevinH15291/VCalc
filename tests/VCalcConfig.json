{
  "testDir": "<testDir>",
  "testedExecutablePaths": {
    "<team id>": "<path_to_vcalc_exe>"
  },
  "runtimes": {
    "<team id>": "<path_to_libvcalcrt.so>"
  },
  "toolchains": {
    "vcalc": [
      {
        "stepName": "vcalc",
        "executablePath": "$EXE",
        "arguments": [
          "$INPUT",
          "$OUTPUT"
          ],
        "output": "vcalc.ll"
      },
      {
        "stepName": "lli",
        "executablePath": "<path_to_lli>",
        "arguments": [
         "$INPUT"
        ],
        "output": "-",
        "usesRuntime": true,
        "usesInStr": true
      }
    ]
  }
}
